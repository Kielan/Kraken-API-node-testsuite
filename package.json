{
  "name": "krakenjesttestsuite",
  "description": "default starter I normally use testing different http clients and kraken api",
  "version": "1.0.0",
  "author": "KielaneLemons <ki.lemons@@gmail.com>",
  "keywords": [
    "kraken"
  ],
  "license": "MIT",
  "scripts": {
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:fonts": "rimraf public/fonts/*",
    "dev:build:client": "webpack --color --display-error-details --config tools/webpack.client.js --bail",
    "dev:browserify": "browserify --debug --transform ./src/layouts/index.js > bundle.js",
    "dev:build:server": "webpack --color --display-error-details --config tools/webpack.server.js --bail",
    "dev:run:server": "NODE_ENV=development node --inspect server.js",
    "dev": "npm run clean:js && npm run clean:fonts && npm run dev:build:client && npm run clean:server && npm run dev:build:server && npm run dev:run:server",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/helper-module-imports": "7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "browserify": "16.2.2",
    "css-loader": "^0.28.7",
    "envify": "4.1.0",
    "node-sass": "^4.7.2",
    "react": "16.4.2",
    "reactify": "1.1.1",
    "run-s": "^0.0.0",
    "sass-loader": "^6.0.6",
    "webpack": "3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "deepmerge": "^2.1.1",
    "express": "^4.16.2",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "^4.3.1",
    "react-zlib-js": "^1.0.4",
    "request-promise": "^4.2.2",
    "style-loader": "^0.23.0"
  }
}
